{
  "name": "sant0ro/chocolatey",
  "description": "The open source HabboWEB API",
  "keywords": [
    "Habbo",
    "RaGEZONE",
    "CMS"
  ],
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/sant0ro/chocolatey/issues",
    "source": "https://github.com/sant0ro/chocolatey/"
  },
  "authors": [
    {
      "name": "Claudio Santoro",
      "email": "claudio.santoro@ieee.org",
      "role": "lead"
    }
  ],
  "type": "project",
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "laravel/lumen-framework": "5.5.*",
    "vlucas/phpdotenv": "^2.4",
    "sofa/eloquence": "5.5.*",
    "rdehnhardt/lumen-maintenance-mode": "^1.2",
    "illuminate/mail": "5.5.*",
    "intervention/image": "^2.3",
    "facebook/graph-sdk": "^5.4",
    "nubs/random-name-generator": "^2.1",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "database/"
    ]
  },
  "scripts": {
    "post-update-cmd": "bash post-install.sh",
    "post-install-cmd": "bash post-install.sh"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
